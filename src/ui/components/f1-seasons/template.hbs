<div>
  Viewing: {{seasonInfo.season}} Season
</div>
<table>
  <thead>
    <th>Name</th>
    <th>Location</th>
    <th>When</th>
  </thead>
  {{#each seasonInfo.Races key="@index" as |race|}}
    <tr>
      <td>{{race.raceName}}</td>
      <td>{{race.Circuit.circuitName}}</td>
      <td>{{race.date}}</td>
    </tr>
  {{/each}}
</table>
